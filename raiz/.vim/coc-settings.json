{
  "snippets.priority": 1,
  "snippets.trace": "verbose",
  "snippets.enableStatusItem": false,
  "snippets.userSnippetsDirectory": "~/.config/nvim/snippets/ultisnips",
  "snippets.ultisnips.enable": true,
  "snippets.ultisnips.usePythonx": true,
  "snippets.ultisnips.directories": [
    "UltiSnips"
  ],
  "snippets.snipmate.enable": true,
  "prettier.enable": true,
  "snippets.textmateSnippetsRoots": [
    "~/.config/vim/snippets/textmate"
  ],
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "scss",
    "dart",
    "java",
    "php",
    "html",
    "python",
    "django",
    "htmldjango",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json"
  ]
}
